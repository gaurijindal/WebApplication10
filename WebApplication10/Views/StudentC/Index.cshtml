@{
    // @model IEnumerable<Student>
    @model IEnumerable<WebApplication10.Models.Student>

    <table>
        <thead>
            <tr>
                <th>ID</th>
                <th>Name</th>
                <th>Enrollment Date</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var student in Model)
            {
                <tr>
                    <td>@student.StudentId</td><br />
                    <td>@student.Name</td><br />
                    <td>@student.EnrollmentDate.ToShortDateString()</td>
                </tr>
            }
        </tbody>
    </table>

}
